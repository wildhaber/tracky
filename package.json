{
  "name": "tracky",
  "version": "1.0.0",
  "description": "a helper module streamlining user interaction into css-classes",
  "main": "src/tracky.js",
  "scripts": {
    "test": "npm run test:eslint && npm run build:dev && npm run test:jasmine",
    "test:local": "npm run test:eslint && npm run build:dev && npm run test:jasmine:formatted",
    "test:eslint": "./node_modules/.bin/eslint ./src/**/*.js",
    "test:jasmine": "./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "test:jasmine:formatted": "node ./test/jasmine/jasmine.js",
    "build": "npm run build:prod && npm run build:dev",
    "build:prod": "webpack --prod",
    "build:dev": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wildhaber/tracky.git"
  },
  "keywords": [
    "tracky.js",
    "tracky",
    "tracker",
    "scroll",
    "device",
    "motions",
    "interactions",
    "ux",
    "ui",
    "css",
    "classes"
  ],
  "author": {
    "name": "Raphael Wildhaber",
    "url": "https://github.com/wildhaber"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wildhaber/tracky/issues"
  },
  "homepage": "https://github.com/wildhaber/tracky#readme",
  "engines": {
    "node": ">= 0.9"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.0.0",
    "expose-loader": "^0.7.1",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "path": "^0.12.7",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "deep-assign": "^2.0.0",
    "lodash.debounce": "^4.0.6"
  }
}
